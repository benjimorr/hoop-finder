<div class="row">
    <div class="col-sm-6">
        <%= link_to 'Back', :back, :class => 'back-btn btn btn-secondary' %>
    </div>
    <div class="col-sm-6">
        <%= link_to 'RSVP', '#', :class => 'rsvp-big btn btn-success' %>
    </div>
</div>

<div class="row">
    <div class="col-sm-12 page-header">
        <h1><%= @game.title %></h1>
    </div>
</div>

<div id="google-map" class="col-sm-12" data-locations="<%= render :partial => 'maps/maps', :locals => { game: @game } %>" data-api-key="<%= ENV["google_api_key"] %>"></div>

<%= javascript_pack_tag 'maps' %>
